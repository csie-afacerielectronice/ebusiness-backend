    "seed:create": "cross-env NODE_ENV=DEV ./node_modules/.bin/sequelize seed:generate",
    "db:migrate": "cross-env NODE_ENV=DEV ./node_modules/.bin/sequelize db:migrate",
    "db:seed": "cross-env NODE_ENV=DEV ./node_modules/.bin/sequelize db:seed:all",
    "test:unit": "cross-env NODE_ENV=TEST jest ./tests/unit --runInBand",
    "test:integration": "cross-env NODE_ENV=TEST jest --runInBand ./tests/integration",
    "test:all": "cross-env NODE_ENV=TEST jest --runInBand --coverage"
  },
  "dependencies": {
